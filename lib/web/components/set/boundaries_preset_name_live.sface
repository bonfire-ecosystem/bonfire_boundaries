{#case List.last(@to_boundaries) |> debug("main boundary to show in composer")}
  {#match {"public", _}}
    <span class="text-lg"><#Icon iconify="ph:globe" class="w-5 h-5" /></span>
    <span class="flex-1 w-full text-left">{l("Public")}</span>
  {#match {"local", _}}
    <span class="text-lg"><#Icon iconify="ph:campfire-bold" class="w-5 h-5" /></span>
    <span class="flex-1 w-full text-left">{l("Local instance")}</span>
  {#match {"mentions", _}}
    <span class="text-lg">ï¹«</span> <span class="flex-1 w-full text-left">{l("Mentions")}</span>
  {#match {"custom", _}}
    <span class="text-lg"><#Icon iconify="ph:gear-fill" class="w-5 h-5" /></span>
    <span class="flex-1 w-full text-left">{l("Custom")}</span>
  {#match {_, item}}
    <span class="text-lg"><#Icon iconify="ph:gear-fill" class="w-5 h-5" /></span>
    <span class="flex-1 w-full text-left">{item}</span>
{/case}
