{#case @preset_boundary}
  {#match {"public", _}}
    <span class="text-lg">🌍</span>{l("Public")}
  {#match {"local", _}}
    <span class="text-lg">🏠</span> {l("Local instance")}
  {#match {"mentions", _}}
    <span class="text-lg">﹫</span> {l("Mentions")}
  {#match {_, name}}
    <span class="text-lg">⛭</span> {name}
  {#match _}
    <span class="text-lg">⛭</span> {l("Custom")}
{/case}
